{
  "name": "localstack-demo",
  "version": "1.0.0",
  "repository": "https://github.com/garydouglasalway/localstack-demo.git",
  "author": "Gary Alway",
  "license": "MIT",
  "scripts": {
    "local:up": "make -C ./infra up",
    "local:down": "make -C ./infra down",
    "format": "prettier src --write",
    "typecheck": "tsc --noEmit",
    "build": "tsc --project tsconfig.json",
    "dev": "serverless offline start --noPrependStageInUrl",
    "lint": "eslint .",
    "purge": "ts-node --files --project tsconfig.json test/purge.cmd"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.83",
    "@types/node": "^16.7.4",
    "@types/ramda": "^0.27.44",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "eslint": "^7.32.0",
    "prettier": "^2.3.2",
    "serverless": "^2.56.0",
    "serverless-localstack": "^0.4.33",
    "serverless-offline": "^8.0.0",
    "serverless-offline-sqs": "^5.0.0",
    "serverless-plugin-typescript": "^1.2.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "@middy/core": "1.5.2",
    "aws-sdk": "^2.978.0",
    "ramda": "^0.27.1"
  }
}
